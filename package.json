{
  "name": "@gravity-haus/gh-common",
  "version": "1.1.14",
  "description": "",
  "author": "apjames93",
  "main": "dist/",
  "types": "dist/",
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "format-with-error": "prettier --list-different \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clean-code": "npm run format && npm run lint",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:ci": "NODE_ENV=test jest --ci --runInBand --reporters=default --reporters=jest-junit --verbose --coverage",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "db:migrate": "npm run db:migrate:dev && npm run db:migrate:test",
    "db:migrate:test": "npx sequelize db:migrate --env test",
    "db:migrate:dev": "npx sequelize db:migrate",
    "db:migrate:undo": "npm run db:migrate:undo:dev --name=$npm_config_name && npm run db:migrate:undo:test --name=$npm_config_name",
    "db:migrate:undo:test": "npx sequelize db:migrate:undo --name=$npm_config_name --env test",
    "db:migrate:undo:dev": "npx sequelize db:migrate:undo --name=$npm_config_name",
    "db:reset": "npx sequelize db:seed --seed reset-db && npm run db:migrate"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.72",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/validator": "13.0.0",
    "dotenv": "^16.0.1",
    "http-request-mock": "^1.7.17",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sequelize-cli": "^5.5.1",
    "ts-jest": "^28.0.4",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.213.0",
    "@aws-sdk/client-sns": "^3.224.0",
    "@hubspot/api-client": "^7.1.2",
    "@slack/web-api": "^6.8.0",
    "@types/sequelize": "^4.28.4",
    "axios": "^0.27.2",
    "chart.js": "^4.0.1",
    "convert-csv-to-json": "^1.4.0",
    "csvtojson": "^2.0.10",
    "esbuild": "^0.14.14",
    "eventbrite": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "pg-hstore": "^2.3.4",
    "phone": "^3.1.32",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.0",
    "sequelize-typescript": "^2.1.3",
    "stripe": "^10.0.0",
    "throttled-queue": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravity-haus/gh-common.git"
  },
  "bugs": {
    "url": "https://github.com/gravity-haus/gh-common/issues"
  },
  "homepage": "https://github.com/gravity-haus/gh-common#readme"
}
